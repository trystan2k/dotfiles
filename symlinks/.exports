export PATH_TO_CUSTOM_APPS="$HOME/Applications"
export PATH_TO_DOWNLOAD="$HOME/Downloads"
export PATH_TO_PROJECTS="$HOME/Documents/Thiago"
export SCREENSHOT_FOLDER=$HOME/Downloads/Screenshots
export TEXT_EDITOR="Visual Studio Code"

export HISTFILE=~/.zsh_history
export HISTSIZE=10000000
export SAVEHIST=10000000

# Disable zsh-tab-title prefix
export ZSH_TAB_TITLE_DEFAULT_DISABLE_PREFIX=true

# This should be the last line of the file
# For local changes
# Don't make edits below this
#shellcheck source=/dev/null
[ -f "$HOME/.exports.local" ] && source "$HOME/.exports.local"